<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <!-- **************************** Vehicle Allocation **********************************-->

     <t t-name="VehicleTemp">
         <table width="100%">
             <tr>
                 <td><h1><center> Vehicle Allocation </center></h1> </td>
             </tr>
         </table>
         <table width="100%">
             <tr>
                 <td width="2%">
                     
                 </td>
                 <td width="96%">
                     <table class="scr" width="100%" border="1">
                          <tr>
                             <td rowspan="2">
                                 
                             </td>
                             <t t-foreach="widget.vehicles" t-as="va">
                                 <td class="pro_qty">
                                     <h4><t t-esc="va['driver_nm']"></t></h4>
                                 </td>
                             </t>
                            <td class="pro_nm" rowspan="2">
                                <h3>Total</h3>
                            </td>
                            <td rowspan="2" colspan="2" width="2%"></td>
                         </tr>
                        <tr>
                            <t t-foreach="widget.vehicles" t-as="va">
                                 <td class="pro_nm">
                                     <h4><t t-esc="va['vehicle_nm']"></t></h4>
                                 </td>
                             </t>
                        </tr>
                        <t t-foreach="widget.vehicle_products" t-as="v_pro">
                            <tr>
                                 <td class="customer_lst">
                                     <h4><t t-esc="v_pro"/></h4>
                                 </td>
                                 <t t-foreach="widget.vehicle_products[v_pro]" t-as="column">
                                    <t t-if="column['driver_lst']">
                                        <t t-foreach="column['driver_lst']" t-as="pro">
                                            <td>
                                                <input step="0.00" min="0" style="text-align: right;" type="number" class="form-control" t-att-id="pro['driver_id']" t-att-data-v_id="pro['vehicle_id']" t-att-value="pro['order_qty']" readonly='readonly'></input>
                                            </td>
                                        </t>
                                    </t>
                                    <t t-foreach="widget.vehicle_pro_qty" t-as="val">
                                        <t t-if="(v_pro == val)">
                                            <td class="right_side">
                                                <h4><t t-esc="widget.vehicle_pro_qty[val]"/></h4>
                                            </td>
                                        </t>
                                    </t>
                                    <td>
                                        <img id="edit" src="/..//manikarnika/web_manikarnika/static/img/gtk-edit.png"/>
                                    </td>
                                    <t t-if="column['product_id']">
                                        <td>
                                            <img id="save" style="visibility:hidden" t-att-data-sr="column['sr_n']" t-att-data-dic="column['product_id']" src="/..//manikarnika/web_manikarnika/static/img/Save-icon2.png"/>
                                        </td>
                                    </t>
                                 </t>
                             </tr>
                        </t>
                        <tr>
                            <td class="item_tot">
                                <h4>Item Total</h4>
                            </td>
                             <t t-foreach="widget.vehicles" t-as="va">
                                <t t-foreach="widget.vehicle_total_qty" t-as="val">
                                    <t t-if="(va['driver_nm'] == val)">
                                        <td class="right_side">
                                            <h4><t t-esc="widget.vehicle_total_qty[val]"/></h4>
                                        </td>
                                    </t>
                                </t>
                            </t>
                        </tr>
                    </table>
                 </td>
                 <td width="2%">
                     
                 </td>
             </tr>
         </table>
    </t> 

    <!-- **************************** Delivery Schedule Manikarnika  **********************************-->

    <t t-name="DeliveryManikarnikaTemp">
         <table width="100%">
             <tr>
                 <td colspan="3"><h1><center>Delivery Schedule Manikarnika </center></h1> </td>
             </tr>
             <tr>
                 <td width="10%">
                     
                 </td>
                 <td width="15%">
                      <input class="form-control orddate" id="orderdate" name="orderdate" placeholder="Select Date" type="date"/>
                 </td>
                 <td>
                      <button type="button" class="searchbtn" id="search">Search</button>
                 </td>
             </tr>
         </table>
         <table width="100%">
             <tr>
                 <td width="2%">
                 </td>
                 <td width="96%" >
                     <table class="scr" width="100%" border="1">
                         <tr>
                             <td rowspan="2">
                                 
                             </td>
                            <t t-foreach="product" t-as="val">
                                <td class="pro_qty">
                                    <h4><t t-esc="product[val]"/></h4>
                                </td>
                            </t>
                            <td rowspan="2" class="pro_nm">
                                <h3>Driver</h3>
                            </td>
                             <td rowspan="2" class="pro_nm">
                                <h3>Shop Total</h3>
                            </td>
                            <td rowspan="2" colspan="2" width="1%"></td>
                         </tr>
                        <tr>
                            <t t-foreach="product" t-as="prod">
                                <td class="pro_nm">
                                    <h4><t t-esc="prod"></t></h4>
                                </td>
                            </t>
                        </tr>
                        <t t-foreach="orders" t-as="order">
                            <tr>
                                 <td class="customer_lst">
                                     <h4><t t-esc="order"/></h4>
                                 </td>
                                 <t t-foreach="orders[order]" t-as="column">
                                      <t t-if="column['product_lst']">
                                          <t t-foreach="column['product_lst']" t-as="pro">
                                              <td>
                                                  <input type="number" min="0" style="text-align: right;" class="form-control" t-att-data-d_qty="pro['d_qty']" t-att-data-sr="pro['serial_no']" t-att-data-pro_id="pro['product_id']" t-att-id="pro['id']" model="morder.tacking.line" t-att-value="pro['qty']" readonly='readonly'></input>
                                              </td>
                                           </t>
                                      </t>
                                      <t t-if="(column['driver_list'])">
                                           <td>
                                               <select class="form-control dri" t-att-id="column['order_id']" model="order.tacking" disabled="disabled">
                                               <option/>
                                               <t t-foreach="column['driver_list']" t-as="driver">
                                                   <t t-if="column.driver_id == driver.dr_id">
                                                       <option t-att-value="driver['dr_id']" t-att-selected=" 'selected'">
                                                         <t t-esc="driver['driver_nm']"/>
                                                       </option>
                                                   </t>
                                                   <t t-if="column.driver_id != driver.dr_id">
                                                       <option t-att-value="driver['dr_id']">
                                                         <t t-esc="driver['driver_nm']"/>
                                                       </option>
                                                   </t>
                                               </t>
                                               <option/>
                                               </select>
                                           </td>
                                      </t>
                                      <t t-if="(column['manik_qty'] == 0)" ><td style="text-align: right"><h4>0</h4></td></t>
                                      <t t-if="column['manik_qty']">
                                         <td class="right_side">
                                             <h4><t t-esc="column['manik_qty']"/></h4>
                                         </td>
                                      </t>
                                  <td>
                                     <input type="image" id="edit" src="/..//manikarnika/web_manikarnika/static/img/gtk-edit.png"/>
                                 </td>
                                 <td>
                                     <input type="image" id="save" t-att-data-ord_id="column['order_id']" style="visibility:hidden"  src="/..//manikarnika/web_manikarnika/static/img/Save-icon2.png"/>
                                 </td>
                                  </t>
                             </tr>
                        </t>
                        <tr>
                            <td class="item_tot">
                                <h4>Item Total</h4>
                            </td>
                            <t t-foreach="item_dic" t-as="val">
                                <td class="right_side">
                                    <h4><t t-esc="item_dic[val]"/></h4>
                                </td>
                            </t>
                        </tr>
                    </table>
                 </td>
                 <td width="2%">
                     
                 </td>
             </tr>
         </table>
    </t>

